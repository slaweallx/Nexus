<!-- BEGIN: ERROR -->
<div class="error">
    <h4><i class="fas fa-times-circle"></i> {PHP.L.Error}</h4>
    <ul class="error-list">
        <!-- BEGIN: ERROR_ROW -->
        <li>{ERROR_ROW_MSG}</li>
        <!-- END: ERROR_ROW -->
    </ul>
</div>
<!-- END: ERROR -->

<!-- BEGIN: WARNING -->
<div class="warning">
    <h4><i class="fas fa-exclamation-triangle"></i> {PHP.L.Warning}</h4>
    <ul class="warning-list">
        <!-- BEGIN: WARNING_ROW -->
        <li>{WARNING_ROW_MSG}</li>
        <!-- END: WARNING_ROW -->
    </ul>
</div>
<!-- END: WARNING -->

<!-- BEGIN: DONE -->
<div class="done">
    <h4><i class="fas fa-check-circle"></i> {PHP.L.Done}</h4>
    <ul class="done-list">
        <!-- BEGIN: DONE_ROW -->
        <li>{DONE_ROW_MSG}</li>
        <!-- END: DONE_ROW -->
    </ul>
</div>
<!-- END: DONE -->